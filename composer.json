    "repositories": [
        {
          "type": "vcs",
          "url": "https://github.com/inceddy/ImageCompare.git"
        }
    ],
    "require": {
        "php": ">=7.0",
        "ramsey/uuid": "^3.7",
        "nategood/httpful": "*",
        "sauce/sausage": ">=0.15.2",
        "php-webdriver/webdriver": "^1.8.2",
        "inceddy/image-compare": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "5.5.*",
        "brianium/paratest": "0.14.*",
        "squizlabs/php_codesniffer": "3.*"
